function find(scale, bags) {
  "use strict";
  
  var c = [];
  
  for (var i=0; i<bags.length; i+=1) {
    c = c.concat(bags[i].getCoins(i+1));
  }
  
  var r = scale(c);
  
  return bags[9 - (r % 10)].id;
}