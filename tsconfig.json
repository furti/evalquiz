{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "out": "dist/evalquiz.js"
  },
  "filesGlob": [
    "ts/**/*.ts",
    "ts/**/*.tsx"
  ],
  "files": [
    "ts/index.d.ts",
    "ts/asyncHelper.ts",
    "ts/attributeInfo.ts",
    "ts/evalquiz.ts",
    "ts/riddleCard.ts",
    "ts/riddleManager.ts"
  ]
}